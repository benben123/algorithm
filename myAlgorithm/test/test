def countMethods(n):
    p = 1
    q = 1
    i = 2
    while i <= n:
        temp = q
        q = q + p
        p = temp
        i += 1
    return q


def countMethod2(n):
    a, b, i = 0, 1, 0
    while i < n:
        a, b = b, a + b
        i + 1
    return b


def recursion(n):
    if n == 0:
        return 1
    if n == 1:
        return 1
    return recursion(n-1) + recursion(n-2)


if __name__ == '__main__':
    val = countMethods(6)
    print val
    val2 = countMethods(6)
    print val2
    val3 = recursion(6)
    print val3
